{
  "compilerOptions": {
    "baseUrl": "./src",
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "jsx": "react",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",

    "lib": ["ESNext", "dom"]
  },
  "include": ["src/**/*"]
}
